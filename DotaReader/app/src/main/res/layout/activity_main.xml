<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:id="@+id/ll_header">


    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout2">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="[DEBUG]     Redownload"
            android:id="@+id/testmatch"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hero List"
        android:id="@+id/btn_herolist"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My matches"
        android:id="@+id/btn_matches"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toRightOf="@+id/btn_herolist"
        android:layout_toEndOf="@+id/btn_herolist" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My Statistics"
        android:id="@+id/btn_prefs"
        android:layout_below="@+id/btn_matches"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toRightOf="@+id/btn_herolist"
        android:layout_toEndOf="@+id/btn_herolist" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Item List"
        android:id="@+id/button2"
        android:layout_below="@+id/btn_herolist"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/btn_matches"
        android:layout_toStartOf="@+id/btn_matches" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:clickable="true"
        android:background="@drawable/bg_lastmatch"
        android:id="@+id/relativeLayout"
        android:layout_below="@+id/button2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/linearLayout2"
        android:layout_margin="10dp">

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lastmatch_heroimg"
            android:layout_marginTop="20dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/gridLayout3" >

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item0"
                android:layout_row="0"
                android:layout_column="0"
                android:src="@drawable/vanguard_lg" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item1"
                android:layout_row="0"
                android:layout_column="1"
                android:src="@drawable/vanguard_lg" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item2"
                android:layout_row="0"
                android:layout_column="2"
                android:src="@drawable/vanguard_lg" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item3"
                android:layout_row="0"
                android:layout_column="3"
                android:src="@drawable/vanguard_lg" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item4"
                android:layout_row="0"
                android:layout_column="4"
                android:src="@drawable/vanguard_lg" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:id="@+id/lm_item5"
                android:layout_row="0"
                android:layout_column="5"
                android:src="@drawable/vanguard_lg" />
        </GridLayout>

        <ImageView
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:id="@+id/lastmatch_heroimg"
            android:src="@drawable/luna_full"
            android:layout_below="@+id/lm_id"
            android:layout_alignLeft="@+id/lm_id"
            android:layout_alignStart="@+id/lm_id"
            android:layout_marginTop="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Last Match"
            android:id="@+id/textView14"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Started 07-07 / 18:40"
            android:id="@+id/lm_started"
            android:layout_alignTop="@+id/lastmatch_heroimg"
            android:layout_toRightOf="@+id/lastmatch_heroimg"
            android:layout_toEndOf="@+id/lastmatch_heroimg"
            android:layout_marginLeft="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Lasted 40:05"
            android:id="@+id/lm_lasted"
            android:layout_below="@+id/lm_started"
            android:layout_toRightOf="@+id/lastmatch_heroimg"
            android:layout_toEndOf="@+id/lastmatch_heroimg"
            android:layout_marginLeft="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="WON / LOST"
            android:id="@+id/lm_condition"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="ID: 000000000"
            android:id="@+id/lm_id"
            android:layout_below="@+id/textView14"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignStart="@+id/textView14" />

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gridLayout3"
            android:id="@+id/lastmatch_grid_stats"
            android:layout_centerHorizontal="true" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="K"
                android:id="@+id/textView16"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="D"
                android:id="@+id/textView26"
                android:layout_row="0"
                android:layout_column="1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="A"
                android:id="@+id/textView27"
                android:layout_row="0"
                android:layout_column="2"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="LH"
                android:id="@+id/textView28"
                android:layout_row="0"
                android:layout_column="3"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="DN"
                android:id="@+id/textView29"
                android:layout_row="0"
                android:layout_column="4"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="XPM"
                android:id="@+id/textView30"
                android:layout_row="0"
                android:layout_column="5"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="GPM"
                android:id="@+id/textView31"
                android:layout_row="0"
                android:layout_column="6"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="00"
                android:id="@+id/lm_kill"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="00"
                android:id="@+id/lm_death"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="00"
                android:id="@+id/lm_assist"
                android:layout_row="1"
                android:layout_column="2"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="000"
                android:id="@+id/lm_lasthit"
                android:layout_row="1"
                android:layout_column="3"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="000"
                android:id="@+id/lm_deny"
                android:layout_row="1"
                android:layout_column="4"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="000"
                android:id="@+id/lm_xpm"
                android:layout_row="1"
                android:layout_column="5"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="000"
                android:id="@+id/lm_gpm"
                android:layout_row="1"
                android:layout_column="6"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />
        </GridLayout>
    </RelativeLayout>

</RelativeLayout>
